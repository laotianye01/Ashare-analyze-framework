        "retry_interval": 10,
        "dependencies": {}
      },
      {
        "name": "get_financial_report",
        "task_class": "FetchFinReport",
        "params": {
          "register": false,
          "convert": true,
          "subject_into": "./data/3main_chart/3finance_charts_main_subject.csv",
          "save": 0,
          "target_db": "FinanceStatement",
          "stock_code": ""
        },
        "retry_interval": 10,
        "dependencies": {}
      }
    ]
  }
